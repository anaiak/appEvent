{
  "colors": {
    "nightBlack": "#0B0B0E",
    "neonPink": "#FF2D95",
    "neonBlue": "#2DF9FF",
    "pureWhite": "#FFFFFF",
    "gray600": "#7A7A7D"
  },
  "typography": {
    "fontFamily": "SF Pro Rounded",
    "title": {
      "size": 28,
      "weight": "bold"
    },
    "heading": {
      "size": 22,
      "weight": "semibold"
    },
    "body": {
      "size": 17,
      "weight": "regular"
    },
    "caption": {
      "size": 15,
      "weight": "regular"
    }
  },
  "spacing": {
    "xs": 4,
    "sm": 8,
    "md": 12,
    "lg": 16,
    "xl": 24,
    "xxl": 32
  },
  "radius": {
    "card": 20,
    "button": 12
  },
  "zIndex": {
    "modal": 1000,
    "overlay": 900,
    "dropdown": 800,
    "header": 700,
    "default": 1
  },
  "animations": {
    "spring": {
      "response": 0.35,
      "dampingFraction": 0.7
    },
    "swipeThreshold": 120,
    "likeThreshold": 40,
    "cardScale": {
      "background": 0.94,
      "middle": 0.97,
      "front": 1.0
    }
  }
} 